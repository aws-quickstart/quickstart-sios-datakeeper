					"VPCCIDR",
					"PrivateSubnet1AId",
					"PrivateSubnet1ACIDR",
                    "PrivateSubnet1AMask",
					"PrivateSubnet2AId",
					"PrivateSubnet2ACIDR",
                    "PrivateSubnet2AMask",
